{
  "name": "hyperlight",
  "version": "0.0.1",
  "description": "The Hyperapp Framework.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.d.ts",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "hyperlight": "dist/hyperlight.js"
  },
  "keywords": [
    "hyperapp",
    "framework",
    "full-stack",
    "web-framework",
    "ssr",
    "ssg"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.20",
    "@types/sane": "^2.0.0",
    "@types/serve-handler": "^6.1.0",
    "@types/ws": "^7.4.0"
  },
  "scripts": {
    "build": "rollup -c"
  },
  "dependencies": {
    "@hyperlight/livereload": "workspace:*",
    "@tinyhttp/app": "^1.1.5",
    "cac": "^6.7.1",
    "chokidar": "github:paulmillr/chokidar",
    "colorette": "^1.2.1",
    "esbuild": "^0.8.30",
    "hyperapp": "^2.0.8",
    "hyperapp-render": "^3.5.0",
    "readdirp": "^3.5.0",
    "serve-handler": "^6.1.3",
    "sirv": "^1.0.10",
    "ws": "^7.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BRA1L0R/hyperlight",
    "directory": "hyperlight"
  }
